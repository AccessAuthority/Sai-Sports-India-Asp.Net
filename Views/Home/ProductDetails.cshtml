                            let currentIndex = 0;
                    const slides = document.querySelectorAll('.carousel-item');
                    const totalSlides = slides.length;

                    function showSlide(index) {
                        if (index >= totalSlides) {
                            currentIndex = 0; // Wrap around to the first image
                        } else if (index < 0) {
                            currentIndex = totalSlides - 1; // Wrap around to the last image
                        } else {
                            currentIndex = index;
                        }
                        updateCarousel();
                    }

                    function updateCarousel() {
                        const carouselContainer = document.querySelector('.carousel-container');
                        const offset = -currentIndex * 100; // Move to the corresponding slide

                        carouselContainer.style.transform = `translateX(${offset}%)`;
                    }

                    function moveSlide(step) {
                        showSlide(currentIndex + step);
                    }

                    // Initialize the carousel to the first slide
                    showSlide(currentIndex);

